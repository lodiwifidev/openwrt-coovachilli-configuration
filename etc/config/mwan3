
config globals 'globals'
	option local_source 'lan'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'
	option logging '1'
	option loglevel 'debug'

config policy 'balanced'
	list use_member 'wan1_m1_w3'
	list use_member 'wan2_m1_w3'
	list use_member 'wan3_m1_w3'
	option last_resort 'unreachable'

config member 'wan1_m1_w3'
	option metric '1'
	option weight '3'
	option interface 'wan1'

config member 'wan1_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan1'

config member 'wan2_m1_w3'
	option metric '1'
	option interface 'wan2'
	option weight '3'

config member 'wan2_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan2'

config member 'wan3_m1_w3'
	option interface 'wan3'
	option metric '1'
	option weight '3'

config member 'wan3_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan3'

config rule 'default_rule_v4'
	option dest_ip '0.0.0.0/0'
	option use_policy 'balanced'
	option family 'ipv4'

config interface 'wan1'
	option enabled '1'
	option family 'ipv4'
	option track_method 'ping'
	option max_ttl '60'
	option timeout '2'
	option recovery_interval '5'
	option down '3'
	option check_quality '1'
	option failure_latency '1000'
	option failure_loss '20'
	option recovery_latency '500'
	option recovery_loss '5'
	option keep_failure_interval '1'
	option reliability '1'
	option count '2'
	option size '8'
	option initial_state 'offline'
	list track_ip '4.2.2.1'
	list track_ip '4.2.2.4'
	option interval '1'
	option failure_interval '3'
	option up '5'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'

config interface 'wan2'
	option enabled '1'
	option family 'ipv4'
	option track_method 'ping'
	option max_ttl '60'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	option check_quality '1'
	option failure_latency '1000'
	option failure_loss '20'
	option recovery_latency '500'
	option recovery_loss '5'
	option keep_failure_interval '1'
	option reliability '1'
	option count '2'
	option size '8'
	option initial_state 'offline'
	list track_ip '4.2.2.2'
	list track_ip '4.2.2.5'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'

config interface 'wan3'
	option enabled '1'
	option family 'ipv4'
	option track_method 'ping'
	option max_ttl '60'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	option check_quality '1'
	option failure_latency '1000'
	option failure_loss '20'
	option recovery_latency '500'
	option recovery_loss '5'
	option keep_failure_interval '1'
	option reliability '1'
	option count '2'
	option size '8'
	option initial_state 'offline'
	list track_ip '4.2.2.3'
	list track_ip '4.2.2.6'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
